<div class="container">
    <div class="row">
        <div class="col-lg-6 col-lg-offset-3">
            <p class="page-title">User Detail</p>

            <table class="table table-bordered">
                <tbody>
                <tr>
                    <td><strong>ID</strong></td>
                    <td><%= user.id %></td>
                </tr>
                <tr>
                    <td><strong>Name</strong></td>
                    <td><%= user.name %></td>
                </tr>
                <tr>
                    <td><strong>Title</strong></td>
                    <td><%= user.title %></td>
                </tr>
                <tr>
                    <td><strong>Email</strong></td>
                    <td><%= user.email %></td>
                </tr>
                </tbody>
            </table>

            <a href="/user" class="btn btn-info"> <i class="glyphicon glyphicon-circle-arrow-left"></i> Back</a>
            <a href="/user/edit/<%= user.id %>" class="btn btn-primary"> <i class="glyphicon glyphicon-edit"></i> Edit</a>
        </div>
    </div>
</div>
