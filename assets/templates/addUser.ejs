<tr data-id="<%= user.id %>" data-model="user">
    <td class="text-center"><%= user.id %></td>
    <td class="text-center">
        <i class="glyphicon glyphicon-<%= user.isOnline ? 'log-in' : 'log-out' %>"
           title="<%= user.isOnline ? 'Logged In' : 'Logged Out' %>"></i>
    </td>
    <td><%= user.name %></td>
    <td><%= user.title %></td>
    <td><%= user.email %></td>
    <td class="text-center">
        <i class="glyphicon glyphicon-<%= user.isAdmin ? 'tower' : 'user' %>"
           title="<%= user.isAdmin ? 'Admin' : 'User' %>"></i>
    </td>
    <td><a href="/user/show/<%= user.id %>" class="btn btn-info"> <i class="glyphicon glyphicon-eye-open"></i> Show</a></td>
    <td><a href="/user/edit/<%= user.id %>" class="btn btn-success"> <i class="glyphicon glyphicon-edit"></i> Edit</a></td>
    <td>
        <form action="/user/destroy/<%= user.id %>">
            <button class="btn btn-danger" type="submit"> <i class="glyphicon glyphicon-remove-circle"></i> Delete</button>
            <input type="hidden" name="_method" value="delete" />
            <input type="hidden" name="_csrf" value="<%= _csrf %>" />
        </form>
    </td>
</tr>
